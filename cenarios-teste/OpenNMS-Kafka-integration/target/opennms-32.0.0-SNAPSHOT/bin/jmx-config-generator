#!/bin/sh

OPENNMS_HOME="/home/epousa/dev/opennms/target/opennms-32.0.0-SNAPSHOT"
OPENNMS_BINDIR="/home/epousa/dev/opennms/target/opennms-32.0.0-SNAPSHOT/bin"

if [ -f "$OPENNMS_HOME/etc/opennms.conf" ]; then
	. "$OPENNMS_HOME/etc/opennms.conf"
fi

exec "$OPENNMS_BINDIR"/runjava -r -- \
	$ADDITIONAL_MANAGER_OPTIONS \
	-Dopennms.home="$OPENNMS_HOME" \
	-jar $OPENNMS_HOME/lib/opennms_jmx_config_generator.jar \
	"$@"
